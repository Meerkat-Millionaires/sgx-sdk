{
  "name": "@switchboard-xyz/sgx",
  "version": "3.0.0",
  "author": "",
  "license": "MIT",
  "description": "A Typescript client to interact with Switchboard V3 SGX quote verifier",
  "files": [
    "lib",
    "src",
    "package.json",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./package.json": "./package.json",
    "./docker": {
      "import": "./lib/esm/SolanaDockerOracle.js",
      "require": "./lib/cjs/SolanaDockerOracle.js"
    }
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/cjs/index.d.ts",
  "scripts": {
    "keypair:create": "shx find ~/.config/solana/id.json || solana-keygen new -s --no-bip39-passphrase --outfile ~/.config/solana/id.json",
    "localnet": "ts-node scripts/localnet",
    "localnet:down": "ts-node scripts/localnet --kill",
    "generate": "node generate-client.js",
    "prebuild": "npm run lint",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc",
    "build": "npm run build:cjs && npm run build:esm",
    "watch": "tsc -p tsconfig.cjs.json --watch",
    "pretest": "npm run build",
    "test": "SOLANA_LOCALNET=1 node ./node_modules/mocha/bin/mocha --loader=ts-node/esm --extension ts --timeout 10000",
    "test:localnet": "SOLANA_LOCALNET=1 node ./node_modules/mocha/bin/mocha --loader=ts-node/esm --extension ts --timeout 60000",
    "test:localnet:mainnet": "SWITCHBOARD_PROGRAM_ID=SW1TCH7qEPTdLsDHRgPuMQjbQxKdH2aBStViMFnt64f SOLANA_LOCALNET=1 node ./node_modules/mocha/bin/mocha --loader=ts-node/esm --extension ts --timeout 60000",
    "posttest": "npm run lint",
    "prepublishOnly": "shx rm -rf lib && npm run build && npm version patch",
    "lint": "gts lint ./src",
    "clean": "gts clean",
    "fix": "gts fix"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.26.0",
    "@coral-xyz/borsh": "^0.26.0",
    "@solana/web3.js": "^1.73.0",
    "@switchboard-xyz/common": "^2.1.33"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/detect-port": "^1.3.2",
    "@types/expect": "^24.3.0",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.18",
    "anchor-client-gen": "^0.25.0",
    "chai": "^4.3.7",
    "chalk": "^4.1.2",
    "detect-port": "^1.5.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "gts": "^3.1.1",
    "lodash": "^4.17.21",
    "mocha": "^10.1.0",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.23",
    "typescript": "^4.9.4"
  },
  "pre-commit": [
    "build"
  ],
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  }
}
